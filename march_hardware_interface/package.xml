  <version>0.0.0</version>
  <description>A bridge between ros_control and the march_hardware</description>

  <maintainer email="software@projectmarch.nl">Project March</maintainer>

  <license>TODO</license>

  <depend>realtime_tools</depend>
  <build_depend>march_shared_resources</build_depend>

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>code_coverage</build_depend>
  <build_depend>roscpp</build_depend>
